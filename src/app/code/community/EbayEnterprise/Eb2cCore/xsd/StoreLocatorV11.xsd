<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<!-- Include schemas -->
	<xs:include schemaLocation="CommonTypes.xsd"/>
	<!-- Define Simple Types -->
	<xs:simpleType name="Simple.address_line">
		<xs:annotation>
			<xs:documentation xml:lang="en-US">Type definition for Simple.address_line simple type.  Simple.address_line is a string(200).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="200"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Simple.address_line_not_empty">
		<xs:annotation>
			<xs:documentation xml:lang="en-US">Type definition for Simple.address_line_not_empty simple type.  Simple.address_line_not_empty is a string(200).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="200"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Simple.attrib_tag">
		<xs:annotation>
			<xs:documentation xml:lang="en-US">Type definition for Simple.attrib_tag simple type. Simple.attrib tag is a string(20). Matching the pattern: "[A-Z][\w_]*" Starts with a capital letter followed by any combination of alpahnumeric characters and underscores.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="60"/>
			<xs:pattern value="[A-Z][\w_]*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Simple.attrib_text_value">
		<xs:annotation>
			<xs:documentation xml:lang="en-US">Type definition for Simple.attrib_text_value simple type. Simple.attrib text_value is a string(1000).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="1000"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Simple.boolean_or_empty">
		<xs:annotation>
			<xs:documentation xml:lang="en-US">Type definition for Simple.boolean_or_empty simple type.  Simple.boolean_or_empty is a boolean or it can be blank or empty.</xs:documentation>
		</xs:annotation>
		<xs:union memberTypes="xs:boolean">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value=""/>
				</xs:restriction>
			</xs:simpleType>
		</xs:union>
	</xs:simpleType>
	<xs:simpleType name="Simple.effective_date_or_empty">
		<xs:annotation>
			<xs:documentation xml:lang="en-US">Type definition for Simple.effective date_or_empty.  Simple.effective date_or_empty is a xs:date, matching the pattern: "[^:Z]*"  Normal date format, but timezone cannot be 'Z'. </xs:documentation>
		</xs:annotation>
		<xs:union>
			<xs:simpleType>
				<xs:restriction base="xs:date">
					<xs:pattern value="[^:Z]*"/>
				</xs:restriction>
			</xs:simpleType>
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value=""/>
				</xs:restriction>
			</xs:simpleType>
		</xs:union>
	</xs:simpleType>
	<xs:simpleType name="Simple.email_address">
		<xs:annotation>
			<xs:documentation xml:lang="en-US">Type definition for Simple.email_address simple type.  Simple.email_address is a string(100).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="Simple.StringType.100"/>
	</xs:simpleType>
	<xs:simpleType name="Simple.environment">
		<xs:annotation>
			<xs:documentation xml:lang="en-US">Type definition for Simple.environment simple type.  Simple.environment is a string(20) The identifier to specify what type of database to be loaded. Examples are DEVELOPMENT, QA1, QA2, PRODUCTION.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="Simple.StringType.20"/>
	</xs:simpleType>
	<xs:simpleType name="Simple.fax_number">
		<xs:annotation>
			<xs:documentation xml:lang="en-US">Type definition for Simple.fax_number simple type.  Simple.fax_number is a string(20).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="Simple.StringType.20"/>
	</xs:simpleType>
	<xs:simpleType name="Simple.feed_update_type">
		<xs:annotation>
			<xs:documentation xml:lang="en-US">Type definition for Simple.feed_update_type simple type.  Simple.feed_update_type is a enumerated xs:string. The enumerated values are "full" and "delta."</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="FULL"/>
			<xs:enumeration value="DELTA"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Simple.latitude">
		<xs:annotation>
			<xs:documentation xml:lang="en-US">Type definition for Simple.latitude simple type.  Simple.latitude is a Decimal with 1 to 2 leading digits and 1 to 6 fractional digits with an optional +/- sign.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:pattern value="[+\-]?\d{1,2}\.\d{1,7}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Simple.locale_city">
		<xs:annotation>
			<xs:documentation xml:lang="en-US">Type definition for Simple.locale_city simple type.  Simple.locale_city is a string(50).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="Simple.StringType.50">
			<xs:minLength value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Simple.locale_code">
		<xs:annotation>
			<xs:documentation xml:lang="en-US">Type definition for Simple.locale_code simple type.  Simple.locale_code is a string(5) with the first 2 characters being lowercase letters, followed by an underscore, an the last 2 characters being uppercase letters. ISO Language Codes are from: ISO 639-1:2002 Part 1: Alpha-2 Language Codes ISO Country Codes are from: ISO 3166-1:2006.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="Simple.StringType.5">
			<xs:pattern value="[a-z][a-z]_[A-Z][A-Z]"/>
			<xs:pattern value="_"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Simple.location_code">
		<xs:annotation>
			<xs:documentation xml:lang="en-US">Type definition for Simple.location_code simple type.  Simple.location_code is a string(40).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="40"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Simple.location_name">
		<xs:annotation>
			<xs:documentation xml:lang="en-US">Type definition for Simple.location_name simple type.  Simple.location_name is a string(200).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="200"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Simple.longitude">
		<xs:annotation>
			<xs:documentation xml:lang="en-US">Type definition for Simple.longitude simple type.  Simple.longitude is a Decimal with 1 to 3 leading digits and 1 to 6 fractional digits with an optional +/- sign.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:pattern value="[+\-]?\d{1,3}\.\d{1,6}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Simple.marker_image_id">
		<xs:annotation>
			<xs:documentation xml:lang="en-US">Type definition for Simple.marker_image_id simple type.  Simple.marker_image_id is a string(256).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="256"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Simple.operation">
		<xs:annotation>
			<xs:documentation xml:lang="en-US">Type definition for Simple.operation simple type.  Simple.operation is a string(10). Allowable values are Operation: UPSERT - update or insert the data; DELETE - delete the data or a blank or empty string.</xs:documentation>
		</xs:annotation>
		<xs:union>
			<xs:simpleType>
				<xs:restriction base="Simple.StringType.10">
					<xs:enumeration value="UPSERT"/>
					<xs:enumeration value="DELETE"/>
				</xs:restriction>
			</xs:simpleType>
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value=""/>
				</xs:restriction>
			</xs:simpleType>
		</xs:union>
	</xs:simpleType>
	<xs:simpleType name="Simple.phone_number">
		<xs:annotation>
			<xs:documentation xml:lang="en-US">Type definition for Simple.phone_number simple type.  Simple.phone_number is a string(15.)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="Simple.StringType.15">
			<xs:maxLength value="15"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Simple.postal_code">
		<xs:annotation>
			<xs:documentation xml:lang="en-US">Type definition for Simple.postal_code simple type.  Simple.postal_code is a string(10).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="Simple.StringType.10">
			<xs:minLength value="1"/>
			<xs:maxLength value="10"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Simple.schedule_name">
		<xs:annotation>
			<xs:documentation xml:lang="en-US">Type definition for Simple.schedule_name simple type.  Simple.schedule_name is a string(100).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="Simple.StringType.100"/>
	</xs:simpleType>
	<xs:simpleType name="Simple.schedule_type">
		<xs:annotation>
			<xs:documentation xml:lang="en-US">Type definition for Simple.schedule_type simple type.  Simple.schedule_type is a string(10). The alowable entries are DAILY and WEEKLY.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="Simple.StringType.10">
			<xs:enumeration value="WEEKLY"/>
			<xs:enumeration value="DAILY"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Simple.state_code">
		<xs:annotation>
			<xs:documentation xml:lang="en-US">Type definition for Simple.state_code simple type.  Simple.state_code is a string(20).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="Simple.StringType.20">
			<xs:minLength value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Simple.status_or_empty">
		<xs:annotation>
			<xs:documentation xml:lang="en-US">Type definition for Simple.status_or_empty simple type.  Simple.status_or_empty is a string(1). Allowable values are Status A - active store, update/insert; Status I - inactive store or a blank or empty string.</xs:documentation>
		</xs:annotation>
		<xs:union>
			<xs:simpleType>
				<xs:restriction base="Simple.StringType.1">
					<xs:enumeration value="A"/>
					<xs:enumeration value="I"/>
				</xs:restriction>
			</xs:simpleType>
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value=""/>
				</xs:restriction>
			</xs:simpleType>
		</xs:union>
	</xs:simpleType>
	<xs:simpleType name="Simple.store_code">
		<xs:annotation>
			<xs:documentation xml:lang="en-US">The identifier provided by eBay Enterprise for an individual or organization that has a contractual relationship with eBay Enterprise in which eBay Enterprise is providing e-commerce, fulfillment or other services.  
Allowable Values: This code must start with an uppercase A-Z or 0-9 followed by any combination of alpahnumeric characters or underscores,
Required: No
Length: 32
Default Value: blank
Restrictions: N/A
Aliases: N/A
</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="32"/>
			<xs:pattern value="[A-Z0-9][\w_]*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Simple.time2400_or_empty">
		<xs:annotation>
			<xs:documentation xml:lang="en-US">Type definition for Simple.time2400_or_empty simple type.  Simple.time2400_or_empty is a string(4).  The allowable values are 4 digit 24 hour time values, 0000-0059, 0100-0059, ... 2400 or a blank or empty string.</xs:documentation>
		</xs:annotation>
		<xs:union>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:pattern value="((([01]\d)|(2[0123]))[012345]\d)|2400"/>
				</xs:restriction>
			</xs:simpleType>
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value=""/>
				</xs:restriction>
			</xs:simpleType>
		</xs:union>
	</xs:simpleType>
	<xs:simpleType name="Simple.decimal_timezone">
		<xs:annotation>
			<xs:documentation xml:lang="en-US">Type definition for Simple.decimal_timezone simple type.  Simple.decimal_timezone is a decimal restricted by a pattern to +14.00 to -13.00 with the the allowable fractions of .0, .5, and 75.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:minInclusive value="-13"/>
			<xs:maxInclusive value="14"/>
			<xs:pattern value="((\+?0*[0-9])|(\+?0*1[0-4])|(-0*[0-9])|(-0*1[0-2]))(.(0*|50*|750*))?"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Simple.type_id">
		<xs:annotation>
			<xs:documentation xml:lang="en-US">Type definition for Simple.type_id simple type.  Simple.type_id is a string(100).  The allowed pattern is: Start with an uppercase A-Z or 0-9 followed by any combination of alphanumeric characters or underscores.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="100"/>
			<xs:pattern value="[A-Z][\w_]*"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- Define Complex Types -->
	<xs:complexType name="Complex.storefront_types">
		<xs:annotation>
			<xs:documentation xml:lang="en-US">Type definition for Complex.storefront_types complex type.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="storefront_type" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en-US">Encapsulates a single storefront_type data for the store.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="type_id" type="Simple.type_id">
							<xs:annotation>
								<xs:documentation xml:lang="en-US">Store Type is a designation used by the partner to group stores.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="operation" type="Simple.operation" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en-US">The operation to perform with the storefront_type data.  The operations are: UPSERT update or insert the attrib data. DELETE - delete the attrib data. The default if blank or null is to UPSERT the data.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Complex.attribs">
		<xs:annotation>
			<xs:documentation xml:lang="en-US">Type definition for Complex.attribs complex type.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="attrib" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en-US">Encapsulates a single attribute data for the store.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="attrib_effective_start_date" type="Simple.effective_date_or_empty" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="en-US">Not currently supported in the store locator application.  Date to start using the schedule data.  Modified XML date format date with optional timezone in HH:MM format. YYYY-MM-DD[(+-)HH:MM] The element can also be empty.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="attrib_effective_end_date" type="Simple.effective_date_or_empty" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="en-US">Not currently supported in the store locator application.  Date to stop using the schedule data.  Modified XML date format date with optional timezone in HH:MM format. YYYY-MM-DD[(+-)HH:MM] The element can also be empty.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="tag_text_value" type="Simple.attrib_text_value">
							<xs:annotation>
								<xs:documentation xml:lang="en-US">The locale specific attrib text value.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="storefront_attrib_tag" type="Simple.attrib_tag" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="en-US">The name of the attribute tag.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="locale_code" type="Simple.locale_code" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="en-US">The locale of the locale specific attrib data.  Locale is a 2-character language_code followed by an underscore, followed by a 2-character country_code.  The locale of the locale specific attrib data.  Locale is a 2- character language_code followed by an underscore, followed by a 2 character country_code. ISO Language Codes are from: ISO 639-1:2002 Part 1: Alpha-2 Language Codes ISO Country Codes are from: ISO 3166-1:2006. </xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="operation" type="Simple.operation" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en-US">The operation to perform with the attrib data.  The operations are: UPSERT update or insert the attrib data. DELETE - delete the attrib data. The default if blank or null is to UPSERT the data.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Complex.schedule_locales">
		<xs:annotation>
			<xs:documentation xml:lang="en-US">Type definition for Complex.schedule_locales complex type.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="schedule_locale" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en-US">Encapsulates locale specific store information for a specific locale.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="schedule_name" type="Simple.schedule_name">
							<xs:annotation>
								<xs:documentation xml:lang="en-US">Not currently supported in the store locator application. The name of the schedule.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="locale_code" type="Simple.locale_code" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="en-US">Not currently supported in the store locator application. The locale of the locale specific schedule data.  Locale is a 2 character language_code followed by an underscore, followed by a 2-character country_code. ISO Language Codes are from: ISO 639-1:2002 Part 1: Alpha-2 Language Codes.
ISO Country Codes are from: ISO 3166-1:2006.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="operation" type="Simple.operation" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en-US">The operation to perform with the schedule_locale data.  The operations are: UPSERT - Update or insert the schedule_locale data. DELETE - delete the schedule_locale data.  The default if blank or null is to UPSERT the data.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Complex.schedules">
		<xs:annotation>
			<xs:documentation xml:lang="en-US">Type definition for Complex.schedules complex type.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="schedule" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en-US">Encapsulates a single set of hours of operation data for the store</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="schedule_effective_start_date" type="Simple.effective_date_or_empty" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="en-US">Not currently supported in the store locator application.  Date to start using the schedule data.  Modified XML date format date with optional timezone in HH:MM format. YYYY-MM-DD[(+-)HH:MM] The element can also be empty.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="schedule_effective_end_date" type="Simple.effective_date_or_empty" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="en-US">Not currently supported in the store locator application.  Date to end using the schedule data.  Modified XML date format date with optional timezone in HH:MM format. YYYY-MM-DD[(+-)HH:MM] The element can also be empty.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="start_display_date" type="Simple.effective_date_or_empty" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="en-US">Not currently supported in the store locator application. Start date for displaying this schedule on the webstore.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="end_display_date" type="Simple.effective_date_or_empty" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="en-US">Not currently supported in the store locator application. Start date for displaying this schedule on the webstore.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="closed_ind" type="Simple.boolean_or_empty">
							<xs:annotation>
								<xs:documentation xml:lang="en-US">Flag to indicate store is closed.  1 - the store is closed 0 - the store is open Default is 0 - the store is open.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="default_open" type="Simple.time2400_or_empty" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="en-US">Default time in hours minutes (24 hour clock) to be used for blank daily open times. Default is 0000.j</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="default_close" type="Simple.time2400_or_empty" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="en-US">Default time in hours minutes (24 hour clock) to be used for blank daily closing times. Default is 0000</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="opening_time_monday" type="Simple.time2400_or_empty" minOccurs="0">
							<xs:annotation>
