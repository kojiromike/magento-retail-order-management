									</xsd:complexType>
								</xsd:element>
								<xsd:element name="Carton" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation>Dimension of a carton of items.</xsd:documentation>
									</xsd:annotation>
									<xsd:complexType>
										<xsd:sequence>
											<xsd:element name="Mass" type="Complex.StringType.ItemWeight">
												<xsd:annotation>
													<xsd:documentation>Weight of item used for display.</xsd:documentation>
												</xsd:annotation>
											</xsd:element>
											<xsd:element name="Packaging" type="Complex.StringType.ItemDimension">
												<xsd:annotation>
													<xsd:documentation>Item dimension within a package.</xsd:documentation>
												</xsd:annotation>
											</xsd:element>
										</xsd:sequence>
									</xsd:complexType>
								</xsd:element>
								<xsd:element name="CartonType" type="Simple.StringType.100" minOccurs="0">
