<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns="http://api.gsicommerce.com/schema/events/1.0.0" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://api.gsicommerce.com/schema/events/1.0.0" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:simpleType name="SourceIdType">
		<xs:annotation>
			<xs:documentation>
                    The type of enterprise id.
                </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="CHANNEL"/>
			<xs:enumeration value="STORE"/>
			<xs:enumeration value="SELLER"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SourceIdTextType">
		<xs:annotation>
			<xs:documentation>
                    The name assigned to the source id.  Examples: 'FOG', 'TRUS'. This will be required
                    as a top level attribute for all Order Service requests.
                </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="24"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="SourceId">
		<xs:simpleContent>
			<xs:annotation>
				<xs:documentation>
                        Represents the channel, store code, or seller.
                    </xs:documentation>
			</xs:annotation>
			<xs:extension base="SourceIdTextType">
				<xs:attribute name="type" type="SourceIdType" use="required"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="KeyValuePair">
		<xs:annotation>
			<xs:documentation>
                Generic Key/Value pair used for partner customizations.
                Within a KeyValuePairList, keys must be unique
            </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Key" type="xs:string">
				<xs:annotation>
					<xs:documentation>Contains a key, such as "Loyalty Program"
Allowable Values: Text string
Required: Yes
Length: TBD
Default Value: blank
Restrictions: N/A
Aliases: N/A</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Value" type="xs:string">
				<xs:annotation>
					<xs:documentation>Contains a value, such as a loyalty program number.
Allowable Values: Text string
Required: Yes
Length: TBD
Default Value: blank
Restrictions: N/A
Aliases: N/A</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="KeyValuePairList">
		<xs:annotation>
			<xs:documentation>
                A list of Generic Key/Value pair used for partner customizations.
                Example use: Loyalty Program
            </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="KeyValuePair" type="KeyValuePair" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Contains a key and a value defined by the cllient.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!--
    <xs:simpleType name="CurrencyAmountType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
                Represents a decimal currency with two decimal points. Sample value: 4.20
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:decimal">
            <xs:fractionDigits value="2"/>
            <xs:minInclusive value="0"/>
        </xs:restriction>
    </xs:simpleType>
    -->
	<xs:simpleType name="CurrencySymbolType">
		<xs:annotation>
			<xs:documentation xml:lang="en">
                A currency symbol value
            </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="0"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CurrencyType">
		<xs:annotation>
			<xs:documentation xml:lang="en">
                A currency value from the set of ISO 4217 currency codes
            </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="3"/>
			<xs:maxLength value="3"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="PriceType">
		<xs:sequence>
			<xs:element name="Currency" type="CurrencyType" nillable="false">
				<xs:annotation>
					<xs:documentation>The type of currency (dollars, francs, etc.) associated with the monetary amount.
Allowable Values: ISO 4217 currency code. http://www.iso.org/iso/support/currency_codes_list-1.htm
Required: Yes
Length: 3
Default Value: blank
Restrictions: N/A
Aliases: N/A</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CurrencySymbol" type="CurrencySymbolType" nillable="false">
				<xs:annotation>
					<xs:documentation>The symbol associated with the currency type, for example $ or £.
Allowable Values: Text string
Required: Yes
Length: TBD
Default Value: blank
Restrictions: N/A
Aliases: N/A</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CurrencyAmount" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation>The monetary amount for the defined currencytype.
Allowable Values: Decimal
Required: Yes
Length: 16.5
Default Value: blank
Restrictions: N/A
Aliases: N/A</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DeltaCurrencyAmount" type="xs:string" nillable="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">This element is used for returns. It captures the cancelled amount of the order (Delta or after picture) from the OMS
Allowable Values: Decimal
Required: No
Length: 16.5
Default Value: blank
Restrictions: N/A
Aliases: N/A</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Taxes" type="TaxesType" minOccurs="0"/>
			<xs:element name="Adjustments" type="PriceAdjustmentList" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This element contains a list of price adjustments for an order.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PromoInfoList" type="PromoInfoListType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PromoInfoListType">
		<xs:sequence>
			<xs:element name="PromoInfo" type="PromoInfoType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FeeListType">
		<xs:sequence>
			<xs:element name="Fee" type="FeePriceType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PriceAdjustmentList">
		<xs:sequence>
			<xs:element name="Adjustment" type="PriceAdjustment" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>This element contains details about a price adjustment.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PriceAdjustment">
		<xs:sequence>
			<xs:element name="Amount" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Specifies the amount of the price adjustment.
Allowable Values: Text string
Required: No
Length: TBD
Default Value: blank
Restrictions: N/A
Aliases: N/A</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DeltaAmount" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">This element captures the Delta (or after picture) consistently from the OMS  
Allowable Values: Text string
Required: No
Length: TBD
Default Value: blank
Restrictions: N/A
Aliases: N/A          		</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Description" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Contains a text description of the adjustment 
Allowable Values: Text string
Required: No
Length: TBD
Default Value: blank
Restrictions: N/A
Aliases: N/A</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Taxes" type="TaxesType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="LocaleCode">
		<xs:annotation>
			<xs:documentation>
                ISO-639 two-letter language code and ISO-3166 two-letter code
            </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:complexType name="DateRange">
		<xs:sequence>
			<xs:element name="From" type="xs:date"/>
			<xs:element name="To" type="xs:date"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="ItemId">
		<xs:annotation>
			<xs:documentation xml:lang="en">
                Represents the identifier for an inventoriable product.
            </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="ChannelId">
		<xs:annotation>
			<xs:documentation>
                The name assigned to the channel.  Examples: 'FOG', 'TRUS'. This will be required
                as a top level attribute for all Service requests.
            </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="24"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="OrderId">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="Gender">
		<xs:restriction base="xs:string">
			<xs:enumeration value="MALE"/>
			<xs:enumeration value="FEMALE"/>
			<xs:enumeration value="UNKNOWN"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="NameType">
		<xs:restriction base="xs:string">
			<xs:maxLength value="60"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="EmailType">
		<xs:restriction base="xs:string">
			<xs:maxLength value="256"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="UserName">
		<xs:sequence>
			<xs:element name="Honorific" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A title you can assign to a person. Typically "Dr.", "Mr.", "Ms." etc.
Allowable Values: Text string.
Required: No
Length: TBD
Default Value: blank
Restrictions: N/A
Aliases: Salutation</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LastName" type="NameType">
				<xs:annotation>
					<xs:documentation>The surname of the person.
Allowable Values: Text string.
Required: Yes
Length: 64
Default Value: blank
Restrictions: N/A
Aliases: N/A  </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MiddleName" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The middle name of the person.
Allowable Values: Text string.
Required: No
Length: TBD
Default Value: blank
Restrictions: N/A
Aliases: N/A  </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FirstName" type="NameType">
				<xs:annotation>
					<xs:documentation>The first name of the person.
Allowable Values: Text string.
Required: Yes
Length: 60
Default Value: blank
Restrictions: N/A
Aliases: N/A</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Suffix" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A title you can append to a person's name. Typically "Jr.", "III" etc.
Allowable Values: Text string.
Required: No
Length: TBD
Default Value: blank
Restrictions: N/A
Aliases: Salutation</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="StoreCodeType">
		<xs:annotation>
			<xs:documentation xml:lang="en">The store code. Internal representation of partner's store.
            </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="6"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="LocaleType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Enumeration of the legal Locale's supported by the Retailing Engine</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="en"/>
			<xs:enumeration value="en_CA"/>
			<xs:enumeration value="en_US"/>
			<xs:enumeration value="fr"/>
			<xs:enumeration value="fr_CA"/>
			<xs:enumeration value="fr_FR"/>
			<xs:enumeration value="de_DE"/>
			<xs:enumeration value="es"/>
			<xs:enumeration value="es_US"/>
			<xs:enumeration value=""/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="EmailAndNameType">
		<xs:sequence>
			<xs:element name="EmailAddress" type="EmailType"/>
			<xs:element name="FirstName" type="xs:string" nillable="true"/>
			<xs:element name="LastName" type="xs:string" nillable="true"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EmailAddressListType">
		<xs:sequence>
			<xs:element name="EmailAndName" type="EmailAndNameType" nillable="false" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="MailToType">
		<xs:sequence>
			<xs:element name="EmailAddressList" type="EmailAddressListType" nillable="false"/>
			<xs:element name="ShareMessage" type="xs:string" nillable="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">
                		The Message that gets sent to email recipients when you share your registry information with them
            		</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="RecordActionType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Insert"/>
			<xs:enumeration value="Update"/>
			<xs:enumeration value="Delete"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="ProductsType">
		<xs:sequence>
			<xs:element name="Product" type="ProductType" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ProductType">
		<xs:sequence>
			<xs:element name="MasterProductId" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Represents the identifier for the product
Allowable Values: Text string
Required: Yes
Length: TBD
Default Value: blank
Restrictions: N/A
Aliases: N/A</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SKU" type="xs:string">
				<xs:annotation>
					<xs:documentation>The retailer's/client's/partner's SKU number or unique item identifier.
Allowable Values: Text string
Required: Yes
Length: TBD
Default Value: blank
Restrictions: N/A
Aliases: N/A</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="UPC" type="UPCType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Allowable Values: Text string
Required: No
Length: 128
Default Value: blank
Restrictions: N/A
Aliases: N/A
</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GlobalId" type="xs:positiveInteger" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The Ebay Enterprise global item ID
Allowable Values: Text string
Required: No
Length: TBD
Default Value: blank
Restrictions: N/A
Aliases: N/A</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ProductTitle" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Name of the product.
Allowable Values: Text string
Required: No
Length: TBD
Default Value: blank
Restrictions: N/A
Aliases: N/A</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ProductDescription" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Explanation of the product.
Allowable Values: Text string
Required: No
Length: TBD
Default Value: blank
Restrictions: N/A
Aliases: N/A</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ProductLink" type="xs:anyURI" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A URL to information about the product.
Allowable Values: Text string
Required: No
Length: TBD
Default Value: blank
Restrictions: N/A
Aliases: N/A</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ProductImageURL" type="xs:anyURI" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A URL to a picture of the product.
Allowable Values: Text string
Required: No
Length: TBD
Default Value: blank
Restrictions: N/A
Aliases: N/A</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ProductAddedDate" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date that the product was added to the order.
Allowable Values: ISO 8601 standard with offset from UTC in datetime data format which is yyyy-mm-ddTHH:mm:ss-hh:mm for e.g. 2012-01-11T14:19:05-06:00
Required: No
Length: TBD
Default Value: blank
Restrictions: N/A
Aliases: N/A</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BrowserId" type="xs:anyURI" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates the type of browser the shopper is using to access the webstore.
Allowable Values: Text string that identifies the type of browser being used to place the order.
Required: No
Length: TBD
Default Value: blank
Restrictions: N/A
Aliases: N/A</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SessionId" type="xs:anyURI" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Unique identifier for the web session. Often used for fraud detection
Allowable Values: Text string used to uniquely identify the browser session when the order was placed.
Required: No
Length: TBD
Default Value: blank
Restrictions: N/A
Aliases: N/A</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Quantity" type="xs:decimal">
				<xs:annotation>
					<xs:documentation>Specifies how many of the product are included in the order.
Allowable Values: Decimal
Required: Yes
Length: TBD
Default Value: blank
Restrictions: N/A
Aliases: N/A</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BasePrice" type="PriceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Unit Price of merchandise before discounts or promotions or adjustments.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Price" type="PriceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Current unit price of merchandise after discounts, promotions, adjustments.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Category" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Not supported.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CustomizationServiceCharge" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Not supported.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Color" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The color of the item purchased.</xs:documentation>
				</xs:annotation>
				<xs:complexType mixed="true">
					<xs:attribute name="id" type="xs:string">
						<xs:annotation>
							<xs:documentation>The ID associated with the color.
Allowable Values: Text string
Required: No
Length: TBD
Default Value: blank
Restrictions: N/A
Aliases: N/A</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="Size" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The size of the item purchased.</xs:documentation>
				</xs:annotation>
				<xs:complexType mixed="true">
					<xs:attribute name="id" type="xs:string">
						<xs:annotation>
							<xs:documentation>The ID associated with the size.
Allowable Values: Text string
Required: No
Length: TBD
Default Value: blank
Restrictions: N/A
Aliases: N/A</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="BrandName" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The brand name assoicated with the item.
Allowable Values: Text string
Required: No
Length: TBD
Default Value: blank
Restrictions: N/A
Aliases: N/A</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StyleNumber" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The number associated with the style type of the item.
Allowable Values: Text string
Required: No
Length: TBD
Default Value: blank
Restrictions: N/A
Aliases: N/A</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="VendorPartNumber" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The part number the vendor has assigned to the item.
Allowable Values: Text string
Required: No
Length: TBD
Default Value: blank
Restrictions: N/A
Aliases: N/A</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BackInStockReasonCode" type="BackInStockReasonCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Code that explains why the item is back in stock.
Allowable Values: Text string
Required: No
Length: TBD
Default Value: blank
Restrictions: N/A
Aliases: N/A</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ItemActionType" type="RecordActionType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Allowable Values: Text string
Required: No
Length: TBD
Default Value: blank
Restrictions: N/A
Aliases: N/A</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Visibility" type="VisibilityType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates if the item is visible in the order.
Allowable Values: 
Visible
Hidden
Required: No
Length: TBD
Default Value: blank
Restrictions: N/A
Aliases: N/A</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Availability" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Allowable Values: Text string
Required: No
Length: TBD
Default Value: blank
Restrictions: N/A
Aliases: N/A</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="QuantityPurchased" type="xs:decimal" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Total number of the product purchased.
Allowable Values: Decimal
Required: No
Length: TBD
Default Value: blank
Restrictions: N/A
Aliases: N/A</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AdditionalValues" type="KeyValuePairList" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Contains additional, client-defined attributes.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
